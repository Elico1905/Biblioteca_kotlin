<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elico.biblioteca">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Biblioteca">
        <activity
            android:name=".Alumnos.ActivityListBook"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.BibliotecaStar">

        </activity>
        <activity
            android:name=".Alumnos.ActivityRegister"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Alumnos.ActivityHome"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.BibliotecaFull"></activity>
        <activity
            android:name=".Login"
            android:exported="true"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

    </application>

</manifest>