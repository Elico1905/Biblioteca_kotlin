<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elico.biblioteca">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_prueba_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono_prueba_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Biblioteca">
        <activity
            android:name=".Admin.ActivityTest"
            android:exported="false" />
        <activity
            android:name=".Alumnos.ActivityMessages"
            android:exported="false" />
        <activity
            android:name=".Alumnos.ActivityInProcess"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.BibliotecaStar" />
        <activity
            android:name=".Alumnos.ActivityListBook"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.BibliotecaStar" />
        <activity
            android:name=".Alumnos.ActivityRegister"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Alumnos.ActivityHome"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.BibliotecaFull" />
        <activity
            android:name=".Login"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>